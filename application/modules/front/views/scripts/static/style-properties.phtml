        <tr><td>stroke-linecap</td><td>"butt", "square", "round"</td><td></td></tr>
        <tr><td>stroke-linecap</td><td>"bevel", "round", "miter"</td><td></td></tr>
        <tr><td>stroke-opacity</td><td> float</td><td>opacité du trait de 0 à 1</td></tr>
        <tr><td>stroke-width</td><td> int</td><td>largeur du trait en pixel, par défaut, 1</td></tr>
        <tr><td>fill</td><td> String, Code hexadécimal</td><td> couleur de remplissage de la zone</td></tr>
        <tr><td>opacity</td><td> float</td><td> opacité de la zone de 0 à 1</td></tr> 
    </tbody>
</table>
